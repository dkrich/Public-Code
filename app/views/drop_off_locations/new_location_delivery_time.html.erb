<div id="new_location_container">
	<div class="new_location_header">
		New Drop-Off Location Time For <%= @drop_off_location.name %>
	</div>
	<%= form_for(@delivery_time) do |f| %>
	  <%= f.error_messages %>
	  <table class="new_location_table" cellspacing=10>
	  	<tr>
	  		<td>
	  			<div class="new_location_text">
	  				Service Type (Lunch, Dinner):
				</div>
	  		</td>
			<td>
				<%= f.text_field :meal_service, :class => "new_location_input" %>
			</td>
		</tr>
	  	<tr>
	  		<td>
	  			<div class="new_location_text">
	  				Time:
				</div>
	  		</td>
			<td>
				<%= f.text_field :delivery_time, :class => "new_location_input" %>
			</td>
		</tr>
		<tr>
			<td colspan=2 align="center">
				<%= f.submit 'Create' %>
			</td>
		</tr>
	  </table>
	<% end %>
</div>